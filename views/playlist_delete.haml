.inner-overlay.modal-content
  .modal-header
    %h4 
      %small Deleting Playlist:
      {{playlist.name}}
  .modal-body.alert-danger
    %h2 You are about to delete this playlist (forever!)
  .modal-footer
    %button.btn.btn-primary{:'ng-click' => "deletePlaylist()"} Delete
    %button.btn{:'ui-sref' => "viewing.playlist.view({id:playlist.id})"} Back
